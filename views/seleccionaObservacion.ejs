<div class="container">
  <h2>Criterio: <%0 observacionBuca %> </h2>
  <table>
    <tr>
       <td>Tipo Reclamo</td>
       <td>Observacion</td>
       <td>Fecha</td>
    </tr>
for(var i=0; i< reclamos.length; i++) {
    if(reclamos[i].observacion == observacionBuca){ %>
       <tr>
           <td><%= reclamos[i].tipoReclamo %></td>
           <td><%= reclamos[i].observacion %></td>
           <td><%= reclamos[i].fecha %></td>
        </tr>
  <% }}%>
  </table>
</div>

